{
   "builds": [
    {
      "src": "vite.config.mjs",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/service",
      "destination": "/services.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/skills",
      "destination": "/skills.html"
    },
     {
      "source": "/contact",
      "destination": "/contact.html"
    }    
  ]
}
